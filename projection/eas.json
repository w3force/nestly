{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.18.0",
      "pnpm": "8.15.0",
      "resourceClass": "default"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "withoutCredentials": false
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nestly-api.up.railway.app",
        "EAS_BUILD_NPM_CACHE_URL": "false"
      }
    },
    "production": {
      "extends": "base",
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nestly-api.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
