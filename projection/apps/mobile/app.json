{
  "expo": {
    "name": "Nestly",
    "slug": "nestly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-appdrawer.png",
    "scheme": "nestly",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.w3force.nestly",
      "icon": "./assets/icon-appdrawer.png"
    },
    "android": {
      "adaptiveIcon": { 
        "foregroundImage": "./assets/icon-appdrawer.png",
        "backgroundColor": "#F5F5F5"
      },
      "package": "com.w3force.nestly"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Nestly"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "208a551f-c9c2-43da-ad5a-8a51790f5128"
      }
    },
    "owner": "w3force"
  }
}
